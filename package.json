{
  "name": "officeiko",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run parcel-build && npm run hugo-build",
    "dev": "concurrently --prefix '{time}-{pid}' 'npm run parcel-watch' 'npm run hugo-watch' --kill-others --kill-others-on-fail",
    "parcel-watch": "rm -rf site/static/assets && parcel watch src/js/index.js -d site/static/assets",
    "parcel-build": "rm -rf site/static/assets && parcel build src/js/index.js -d site/static/assets",
    "hugo-watch": "hugo server -w -D -s site --bind 0.0.0.0 --config site/config.toml,site/market.toml",
    "hugo-build": "rm -rf site/public && hugo -s site --config site/config.toml,site/market.toml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyasu1/officeiko-2018.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyasu1/officeiko-2018/issues"
  },
  "homepage": "https://github.com/kyasu1/officeiko-2018#readme",
  "devDependencies": {
    "autoprefixer": "^9.1.5",
    "concurrently": "^4.0.1",
    "cssnano": "^4.1.4",
    "postcss": "^7.0.4",
    "postcss-modules": "^1.3.2",
    "sass": "^1.14.1",
    "tailwindcss": "^0.6.6"
  }
}
