{
  "name": "officeiko",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run hugo-build",
    "dev": "concurrently --prefix '{time}-{pid}' 'npm run rollup-watch' 'npm run hugo-watch' --kill-others --kill-others-on-fail",
    "rollup-watch": "rollup -c -w",
    "rollup-build": "rm -rf site/static/assets && NODE_ENV=production rollup -c",
    "hugo-watch": "hugo server -w -D --verbose -s site --bind 0.0.0.0 --configDir site --config config.toml,market.toml,ticket.toml",
    "hugo-build": "rm -rf site/public && hugo -s site --configDir site --config config.toml,market.toml,ticket.toml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyasu1/officeiko-2018.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyasu1/officeiko-2018/issues"
  },
  "homepage": "https://github.com/kyasu1/officeiko-2018#readme",
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "concurrently": "^4.1.2",
    "cssnano": "^4.1.10",
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.4",
    "node-elm-compiler": "^5.0.4",
    "rollup": "^2.35.1",
    "rollup-plugin-elm": "^2.0.2",
    "rollup-plugin-postcss": "^4.0.0",
    "sass": "^1.26.2",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@tailwindcss/ui": "^0.4.0",
    "autoprefixer": "^10.1.0",
    "core-js": "^3.6.4",
    "dist": "^0.1.2",
    "glightbox": "^2.0.5",
    "postcss": "^8.2.1",
    "tailwindcss": "^2.0.2"
  }
}