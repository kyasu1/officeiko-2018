{
  "name": "officeiko",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run hugo-build",
    "dev": "concurrently --prefix '{time}-{pid}' 'npm run parcel-watch' 'npm run hugo-watch' --kill-others --kill-others-on-fail",
    "parcel-watch": "parcel watch src/js/index.js -d site/static/assets",
    "parcel-build": "rm -rf site/static/assets && parcel build src/js/index.js -d site/static/assets",
    "hugo-watch": "hugo server -w -D --verbose -s site --bind 0.0.0.0 --configDir site --config config.toml,market.toml,ticket.toml",
    "hugo-build": "rm -rf site/public && hugo -s site --configDir site --config config.toml,market.toml,ticket.toml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kyasu1/officeiko-2018.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kyasu1/officeiko-2018/issues"
  },
  "homepage": "https://github.com/kyasu1/officeiko-2018#readme",
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "concurrently": "^4.1.2",
    "cssnano": "^4.1.10",
    "elm": "^0.19.1-3",
    "elm-hot": "^1.1.4",
    "node-elm-compiler": "^5.0.4",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.25",
    "postcss-modules": "^1.4.1",
    "sass": "^1.24.0",
    "stylelint": "^10.1.0",
    "stylelint-config-standard": "^18.3.0",
    "tailwindcss": "^1.1.4"
  },
  "dependencies": {
    "core-js": "^3.6.0",
    "highlight.js": "^9.17.1"
  }
}
